    "def grover_oraculo(n,s):\n",
    "    oracle = QuantumCircuit(n+1)\n",
    "    \n",
    "    # Invertimos los cubits para los que s tenga un bit a 0\n",
    "    temp = s\n",
    "    for b in range(n):\n",
    "        if(temp&1 == 0):\n",
    "            oracle.x(b)\n",
    "        temp >>= 1\n",
    "    # Aplicamos una CCNOT \n",
    "    oracle = oracle.compose(MCMT('x',n,1))\n",
    "    # Invertimos los cubits para los que s tenga un bit a 0\n",
    "    temp = s\n",
    "    for b in range(n):\n",
    "        if(temp&1 == 0):\n",
    "            oracle.x(b)\n",
    "        temp >>= 1\n",
    "        \n",
    "    return oracle\n",
    "\n",
    "grover_oraculo(4, 5).draw(\"mpl\")"
